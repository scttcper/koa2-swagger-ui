{
  "name": "koa2-swagger-ui",
  "version": "0.0.0-placeholder",
  "description": "Swagger UI middleware for koa",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "ts-node test/example.ts",
    "lint": "eslint lib/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix lib/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "build": "del dist && tsc -p tsconfig.build.json",
    "postbuild": "cp lib/index.hbs dist && cp lib/favicon-32x32.png dist && cp lib/favicon-16x16.png dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@types/koa": "^2.0.49",
    "handlebars": "^4.1.2",
    "lodash": "^4.17.15",
    "read-pkg-up": "^6.0.0"
  },
  "peerDependencies": {
    "koa": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/koa-router": "7.0.42",
    "@types/lodash": "4.14.138",
    "@types/node": "12.7.3",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "del-cli": "2.0.0",
    "eslint": "6.2.2",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.17.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "koa": "2.7.0",
    "koa-router": "7.4.0",
    "semantic-release": "15.13.24",
    "supertest": "4.0.2",
    "swagger-ui-dist": "3.23.6",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.6.2"
  },
  "release": {
    "branch": "master"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/scttcper/koa2-swagger-ui",
  "repository": "scttcper/koa2-swagger-ui",
  "keywords": [
    "swagger",
    "docs",
    "swagger-ui",
    "koa",
    "koa2",
    "middleware"
  ],
  "license": "MIT"
}
